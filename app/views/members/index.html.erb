<h2>
    <%= link_to "支局メンバー", new_member_path %>
    <% @members.each do |member| %>
    <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h2 class="card-title">
        <%= link_to member.name, edit_member_path(member) %>
      </h2>
      <h6 class="card-text">
        <%= member.created_at %>
      </h6>
      <h6>
        <%= link_to "削除", member_path(member), method: :delete %>
      </h6>
    </div>
    </div>
    <% end %>
</h2>
